#include <bits/stdc++.h>
using namespace std;

int searchElement(int arr[], int n, int x){
    for(int i=0;i<n;i++){
        if(arr[i]== x){
            return i;
        }
    }
    return -1;
}
int main() {
    int n;
    cin>>n;
    int x;
    cin>>x;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
     cout<<searchElement(arr,n,x)<<endl;

}

5
6
1 3 4 6 90
